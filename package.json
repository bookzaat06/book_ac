{
  "name": "book_ac",
  "version": "0.1.0",
  "private": true,
  "main": "main/main.js",
  "author": "RBFraphael",
  "description": "Electron + NextJS example project",
  "scripts": {
    "dev": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev\" \"electron .\"",
    "build": "next build && electron-builder",
    "package": "electron-builder -p always",
    "start": "next start",
    "lint": "next lint"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "bookzaat06",
        "repo": "book_ac"
      }
    ]
  },
  "dependencies": {
    "electron-serve": "^1.3.0",
    "electron-updater": "^6.1.8",
    "next": "14.1.0",
    "react": "^18",
    "react-dom": "^18",
    "update-electron-app": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "electron": "^29.0.1",
    "electron-builder": "^24.12.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
